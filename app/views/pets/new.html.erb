<div class="container">
<%= link_to "Back to shelter dashboard", shelter_path(@shelter), class: "btn btn-primary mb-4" %>
<%= simple_form_for [@shelter, @pet]  do |f| %>
  <%= f.input :name %>
  <%= f.input :age %>
  <%= f.input :animal_type %>
  <%= f.input :price_per_day %>
  <%= f.input :gender %>
  <%= f.input :breed %>
  <%= f.input :description %>
  <%= f.submit %>
<% end %>
